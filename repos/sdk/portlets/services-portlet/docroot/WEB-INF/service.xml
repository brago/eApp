<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.rknowsys.eapp">
	<author>rknowsys</author>
	<namespace>hrm</namespace>


<entity name="SalaryComponent" table="salary_component" data-source="anotherDataSource" local-service="true" remote-service="false" session-factory="anotherSessionFactory" tx-manager="anotherTransactionManager">

        <!-- PK fields -->

        <column name="id" type="long" primary="true" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="groupId" type="long" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />
        <column name="userId" type="long" />
        <!-- Other fields -->

        <column name="componentName" type="String" />
        <column name="type" type="boolean" />
        <column name="onlyCTC" type="boolean" />
        <column name="valueType" type="String" />

        <!-- Order -->

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
                <finder-column name="groupId" />
        </finder>
    </entity>
    
    
	<entity name="JobTitles" table="job_titles" data-source="anotherDataSource" local-service="true" remote-service="false" session-factory="anotherSessionFactory" tx-manager="anotherTransactionManager">

        <!-- PK fields -->

        <column name="id" type="long" primary="true" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="groupId" type="long" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />
        <column name="userId" type="long" />
        <!-- Other fields -->

        <column name="title" type="String" />
        <column name="description" type="String" />
        <column name="notes" type="String" />
        <column name="specification" type="String" />

        <!-- Order -->

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
                <finder-column name="groupId" />
        </finder>
    </entity>
    
    
    
<entity name="JobCategory" table="job_category" data-source="anotherDataSource" local-service="true" remote-service="false" session-factory="anotherSessionFactory" tx-manager="anotherTransactionManager">

        <!-- PK fields -->

        <column name="id" type="long" primary="true" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="groupId" type="long" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />
        <column name="userId" type="long" />
        <!-- Other fields -->

        <column name="jobcategory" type="String" />
       
        <!-- Order -->

       <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
                <finder-column name="groupId" />
        </finder>
    </entity>
	
	
</service-builder>