<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.rknowsys.eapp.hrm.model.impl.JobCategoryModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="userId" />
			<basic name="jobcategory" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.rknowsys.eapp.hrm.model.impl.JobTitlesModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="userId" />
			<basic name="title" />
			<basic name="description" />
			<basic name="notes" />
			<basic name="specification" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.rknowsys.eapp.hrm.model.impl.JobCategoryImpl" name="JobCategory">
		<table name="job_category" />
		<attributes />
	</entity>
	<entity class="com.rknowsys.eapp.hrm.model.impl.JobTitlesImpl" name="JobTitles">
		<table name="job_titles" />
		<attributes />
	</entity>
</entity-mappings>